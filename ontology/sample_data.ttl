@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org#> .

# Instances de Electronics
ex:Smartphone1 rdf:type ex:Electronics ;
               ex:hasBrand "BrandA" ;
               ex:hasPrice "299" ;
               ex:hasSpecification "Specification details here" .

ex:Laptop1 rdf:type ex:Electronics ;
           ex:hasBrand "BrandB" ;
           ex:hasPrice "799" ;
           ex:hasSpecification "Specification details here" .

